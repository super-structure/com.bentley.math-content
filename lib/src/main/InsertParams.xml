<?xml version="1.0" encoding="UTF-8"?>
<parameters xmlns:if="ant:if" xmlns:unless="ant:unless">
    <!-- list of parameters passed to XSLT -->
    <param
        name="MATH-PROC"
        expression="${args.mathml.processing}"
        if:set="args.mathml.processing"
        />
    <param
        name="EQN-PREFIX"
        expression="${args.eqnlink.style}"
        if:set="args.eqnlink.style"
        />
<param 
        name="MATHML-SVG-FILE"
        expression="${args.mathml.svg2file}"
        if:set="args.mathml.svg2file"
        />
</parameters>

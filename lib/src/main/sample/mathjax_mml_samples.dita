        <section>
            <title>A Rogers-Ramanujan Identity</title>
            <equation-figure>
                <equation-block>
                    <mathml>
                        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML" display="block">
                            <m:mn>1</m:mn>
                            <m:mo>+</m:mo>
                            <m:mfrac>
                                <m:msup>
                                    <m:mi>q</m:mi>
                                    <m:mn>2</m:mn>
                                </m:msup>
                                <m:mrow>
                                    <m:mo stretchy="false">(</m:mo>
                                    <m:mn>1</m:mn>
                                    <m:mo>&#x2212;<!-- − --></m:mo>
                                    <m:mi>q</m:mi>
                                    <m:mo stretchy="false">)</m:mo>
                                </m:mrow>
                            </m:mfrac>
                            <m:mo>+</m:mo>
                            <m:mfrac>
                                <m:msup>
                                    <m:mi>q</m:mi>
                                    <m:mn>6</m:mn>
                                </m:msup>
                                <m:mrow>
                                    <m:mo stretchy="false">(</m:mo>
                                    <m:mn>1</m:mn>
                                    <m:mo>&#x2212;<!-- − --></m:mo>
                                    <m:mi>q</m:mi>
                                    <m:mo stretchy="false">)</m:mo>
                                    <m:mo stretchy="false">(</m:mo>
                                    <m:mn>1</m:mn>
                                    <m:mo>&#x2212;<!-- − --></m:mo>
                                    <m:msup>
                                        <m:mi>q</m:mi>
                                        <m:mn>2</m:mn>
                                    </m:msup>
                                    <m:mo stretchy="false">)</m:mo>
                                </m:mrow>
                            </m:mfrac>
                            <m:mo>+</m:mo>
                            <m:mo>&#x22EF;<!-- ⋯ --></m:mo>
                            <m:mo>=</m:mo>
                            <m:munderover>
                                <m:mo>&#x220F;<!-- ∏ --></m:mo>
                                <m:mrow class="MJX-TeXAtom-ORD">
                                    <m:mi>j</m:mi>
                                    <m:mo>=</m:mo>
                                    <m:mn>0</m:mn>
                                </m:mrow>
                                <m:mrow class="MJX-TeXAtom-ORD">
                                    <m:mi mathvariant="normal">&#x221E;<!-- ∞ --></m:mi>
                                </m:mrow>
                            </m:munderover>
                            <m:mfrac>
                                <m:mn>1</m:mn>
                                <m:mrow>
                                    <m:mo stretchy="false">(</m:mo>
                                    <m:mn>1</m:mn>
                                    <m:mo>&#x2212;<!-- − --></m:mo>
                                    <m:msup>
                                        <m:mi>q</m:mi>
                                        <m:mrow class="MJX-TeXAtom-ORD">
                                            <m:mn>5</m:mn>
                                            <m:mi>j</m:mi>
                                            <m:mo>+</m:mo>
                                            <m:mn>2</m:mn>
                                        </m:mrow>
                                    </m:msup>
                                    <m:mo stretchy="false">)</m:mo>
                                    <m:mo stretchy="false">(</m:mo>
                                    <m:mn>1</m:mn>
                                    <m:mo>&#x2212;<!-- − --></m:mo>
                                    <m:msup>
                                        <m:mi>q</m:mi>
                                        <m:mrow class="MJX-TeXAtom-ORD">
                                            <m:mn>5</m:mn>
                                            <m:mi>j</m:mi>
                                            <m:mo>+</m:mo>
                                            <m:mn>3</m:mn>
                                        </m:mrow>
                                    </m:msup>
                                    <m:mo stretchy="false">)</m:mo>
                                </m:mrow>
                            </m:mfrac>
                            <m:mo>,</m:mo>
                            <m:mspace width="1em"/>
                            <m:mspace width="1em"/>
                            <m:mrow>
                                <m:mtext>for&#xA0;</m:mtext>
                                <m:mrow class="MJX-TeXAtom-ORD">
                                    <m:mrow class="MJX-TeXAtom-ORD">
                                        <m:mo stretchy="false">|</m:mo>
                                    </m:mrow>
                                    <m:mi>q</m:mi>

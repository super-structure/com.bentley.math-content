                            <m:mfrac>
                                <m:msup>
                                    <m:mi>e</m:mi>
                                    <m:mrow class="MJX-TeXAtom-ORD">
                                        <m:mo>&#x2212;<!-- − --></m:mo>
                                        <m:mn>2</m:mn>
                                        <m:mi>&#x03C0;<!-- π --></m:mi>
